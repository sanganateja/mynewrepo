pipeline {
 agent any
 stages {
        stage('Build') {
            steps {
                echo 'Building'
			}	
        }
		stage('Build Docker Image'){
	        steps {
			    sh 'docker build -t sangana/new-nginx:${BUILD_NUMBER} .'
	        }
		}
        stage ('test'){
	        steps {
		        echo 'testing'
			}
	    }
    }
}	
